<h1>New start sign</h1>

<form action="<%= starsigns_path %>" method="post">
    <label>
        Name:
        <input name="name" required>
    </label>

    <label>
        Image:
        <input type="url" name="image">
    </label>

    <label>
        Planet:
        <input name="planet" required>
    </label>

   
    <button>Create star sign</button>
</form>